{
	"name": "@stdremin/serializer",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"description": "Простой datamaper для описания и конвертации объектов в моделли для ts",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview"
	},
	"keywords": [
		"datamaper",
		"ts models",
		"TypeScript models"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/st-dremin/serializer-ts.git"
	},
	"author": "StanislavDremin",
	"main": "./dist/serializer.umd.cjs",
	"module": "./dist/serializer.js",
	"types": "./dist/serializer.d.ts",
	"exports": {
		".": {
			"import": "./dist/serializer.js",
			"require": "./dist/serializer.umd.cjs"
		}
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"@faker-js/faker": "^8.1.0",
		"@jest-decorated/core": "^0.1.7",
		"@types/jest": "^29",
		"@types/node": "^18",
		"jest": "^29",
		"ts-jest": "^29",
		"typescript": "^4",
		"vite": "^4",
		"vite-plugin-dts": "^3"
	},
	"dependencies": {
		"reflect-metadata": "^0.1.13"
	},
	"engines": {
		"node": "^18 || ^16 || ^14.19",
		"npm": ">= 6.13.4",
		"yarn": ">= 1.21.1"
	}
}
